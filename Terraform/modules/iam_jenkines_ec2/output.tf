############################################
# OUTPUTS
############################################

output "instance_id" {
  value = aws_instance.this.id
}

output "public_ip" {
  value = aws_instance.this.public_ip
}

output "public_dns" {
  value = aws_instance.this.public_dns
}

output "jenkins_url" {
  value = "http://${aws_instance.this.public_dns}:8080"
}

output "security_group_id" {
  value = aws_security_group.this.id
  }